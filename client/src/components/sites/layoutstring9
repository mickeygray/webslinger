                </select>
                <div className='grid-2'>
                  <div>
                    {" "}
                    <input
                      type='radio'
                      name='position'
                      value='false'
                      checked={D.position === "false"}
                      onChange={(e) =>
                        setD({ ...D, [e.target.name]: e.target.value })
                      }
                    />
                    Left
                  </div>
                  <div>
                    {" "}
                    <input
                      type='radio'
                      name='position'
                      value='true'
